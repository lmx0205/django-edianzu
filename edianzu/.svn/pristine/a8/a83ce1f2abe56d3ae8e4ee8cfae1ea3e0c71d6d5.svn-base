   <script>
       function select(){
            document.querySelector('.filter-box').style.display='block'
       };
       var chooseList = {}
       function choose1(e){
           $('.filter-container .item1 span').removeClass('cur')
           $(e).addClass('cur')
           chooseContent1 =  $(e)[0]['textContent']
           chooseList['choose1']=chooseContent1
           console.log(chooseList)
       }
       function choose2(e) {
            $('.filter-container .item2 span').removeClass('cur')
            $(e).addClass('cur')
            chooseContent2 = $(e)[0]['textContent']
            chooseList['choose2'] = chooseContent2
            console.log(chooseList)
        }
        function submit(){
            $('.product-list').remove()
            $.ajax({
                type: "get",
                url: "http://localhost:8000/goods/category/choose/",
                dataType: "text",

                data: chooseList,
                success: function (data) {
                    console.log(data)
                    data = JSON.parse(data)
                    
                    for (index in data){
                        $('.produce').append(`
                        <div data-v-1d568bf2="" data-v-0a1d19f2="" class="product-list">
                                <div data-v-1d568bf2="" class="product-item">
                                    <div data-v-1d568bf2="" class="img-box"><img data-v-1d568bf2="" alt="" src="/`+data[index].image+`" lazy="loaded"> </div>
                                    <div data-v-1d568bf2="" class="content"><a data-v-1d568bf2="" href="https://m.edianzu.com/wap/product/115064"
                                            class="" title="`+ data[index].name +`">
                                            <p data-v-1d568bf2="" class="title">
                                                <span data-v-1d568bf2="" class="content-tip">`+ data[index].keywords +`</span>
                                                `+ data[index].details+`
                                            </p>
                                            <div data-v-1d568bf2="" class="tip-box">
                                                
                                            </div>
                                            <p data-v-1d568bf2="" class="price"><span data-v-1d568bf2="" class="price-sign">￥</span>
                                                <span class="pricenum">`+ data[index].price +`</span> 
                                            </p>
                                        </a></div>
                                    
                                </div>
                                
                                
                                
                            `)}

                    
                }
            })
            document.querySelector('.filter-box').style.display = 'none'
        }
        function chongzhi(){
            $('.filter-container .item1 span').removeClass('cur')
            $('.filter-container .item2 span').removeClass('cur')
            $('.buxian').addClass('cur')
        }
        function back(){
            document.querySelector('.filter-box').style.display = 'none'
        }
        
       
    </script>

